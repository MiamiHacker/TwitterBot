<%- include("./partials/header.ejs") %>
<form action="test" method="post">

    <input type="text" name="text" id="text"> <br>
    <input type="submit" value="submit">

</form>

<form id="testForm" enctype="application/x-www-form-urlencoded">
    <input type="color" name="testColor" >
    <input type="text" name="testText" value="textValye">
    <input type="submit" value="submit">
</form>

<script>
    let testbutton = document.createElement('button');
    testbutton.innerText = 'test'

    let testForm = document.getElementById('testForm');

    testForm.addEventListener('submit', async(e)=>{
        console.log(testForm.value)
        e.preventDefault()

        let formData = new FormData(testForm);
        let urlEncoded = new URLSearchParams(formData);

        let options = {
            method:'post',
            body:urlEncoded,
        }

        let fetcher = await (await fetch('/popup_register',options)).text();
        console.log(fetcher)
    });

    document.body.append(testbutton);
    
</script>